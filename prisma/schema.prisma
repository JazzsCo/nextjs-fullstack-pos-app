generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model addon {
  id          Int     @id @default(autoincrement())
  addon_name  String
  price       Int
  is_avilable Boolean @default(true)
}

model addon_addon_categories {
  id           Int @id @default(autoincrement())
  addon_id     Int
  addon_cat_id Int
}

model addon_categories {
  id            Int     @id @default(autoincrement())
  category_name String
  is_required   Boolean @default(true)
}

model location_menus {
  id           Int      @id @default(autoincrement())
  location_id  Int
  menu_id      Int
  is_available Boolean? @default(true)
}

model locations {
  id   Int    @id @default(autoincrement())
  name String
}

model menu_categories {
  id            Int    @id @default(autoincrement())
  category_name String
}

model menu_images {
  id  Int    @id @default(autoincrement())
  url String
}

model menus {
  id        Int     @id @default(autoincrement())
  name      String
  price     Int
  image_url String?
}

model menus_addon_categories {
  id           Int @id @default(autoincrement())
  menus_id     Int
  addon_cat_id Int
}

model menus_menu_categories {
  id          Int @id @default(autoincrement())
  menus_id    Int
  category_id Int
}

model menus_menu_images {
  id             Int @id @default(autoincrement())
  menus_id       Int
  menu_images_id Int
}

model menus_order {
  id          Int      @id @default(autoincrement())
  name        String
  price       Int
  is_avilable Boolean? @default(false)
}

model users {
  id       Int    @id @default(autoincrement())
  name     String
  email    String
  password String
}
